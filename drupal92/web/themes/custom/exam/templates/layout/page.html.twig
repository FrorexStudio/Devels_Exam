<div class="layout-container">

  <header role="banner" class="header">
    <div class="container">
      <div class="navbar">
        {{ page.navigation }}
      </div>
    </div>
  </header>

  {{ page.highlighted }}

  <main role="main">
    <a id="main-content" tabindex="-1"></a>

    <div class="layout-content">
      {{ page.content }}
    </div>

  </main>

  <footer role="contentinfo" class="footer">
    <div class="container">
      {{ page.footer }}
    </div>
  </footer>

</div>

